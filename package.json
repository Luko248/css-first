{
  "name": "@depthark/css-first",
  "description": "MCP server for CSS first search",
  "license": "MIT",
  "version": "0.1.0",
  "bugs": {
    "url": "https://github.com/luko248/mcp-css-first/issues"
  },
  "homepage": "https://github.com/luko248/mcp-css-first#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luko248/mcp-css-first.git"
  },
  "keywords": [
    "css",
    "modelcontextprotocol",
    "mcp",
    "css",
    "mdn",
    "web-development"
  ],
  "bin": {
    "mcp-css-first": "dist/cli.js"
  },
  "files": [
    "dist/cli.js",
    "src"
  ],
  "scripts": {
    "build": "ncc build src/index.ts --minify --out dist/ && echo '#!/usr/bin/env node' | cat - dist/index.js > dist/cli.js && chmod +x dist/cli.js",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "start": "node dist/cli.js",
    "update-features": "tsx src/scripts/updateFeatures.ts",
    "discover-features": "tsx src/services/css/featureDiscovery.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "~1.13",
    "zod": "~3.25"
  },
  "devDependencies": {
    "@eslint/js": "~9.29",
    "@types/node": "^24",
    "@vercel/ncc": "^0.38.3",
    "eslint": "~9.29",
    "typescript": "~5.8",
    "typescript-eslint": "~8.35"
  }
}